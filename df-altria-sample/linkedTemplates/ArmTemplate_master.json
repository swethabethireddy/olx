{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-altria-sample"
		},
		"AzureBlobStorage_inputlnk_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_inputlnk'"
		},
		"AzureDataLakeStorage_gen2lnk_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_gen2lnk'"
		},
		"AzureDataLakeStorage_gen2lnk_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sweadlsgen2.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-altria-sample_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_inputlnk_connectionString": {
						"value": "[parameters('AzureBlobStorage_inputlnk_connectionString')]"
					},
					"AzureDataLakeStorage_gen2lnk_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_gen2lnk_accountKey')]"
					},
					"AzureDataLakeStorage_gen2lnk_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_gen2lnk_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}